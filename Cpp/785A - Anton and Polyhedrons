#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    cin>>n;
    int count=0;
    while(n--)
    {
        string a;
        cin>>a;
        if(a=="Icosahedron")
        {
            count+=20;
        }
        if(a=="Cube")
        {
            count+=6;
        }
        if(a=="Tetrahedron")
        {
            count+=4;
        }
        if(a=="Dodecahedron")
        {
            count+=12;
        }
        if(a=="Octahedron")
        {
            count+=8;
        }
    }
    cout<<count;
}
